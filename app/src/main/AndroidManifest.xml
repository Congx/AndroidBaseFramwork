<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidbaseframwork">

    <application
        android:name="com.base.androidbaseframwork.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.base.androidbaseframwork.ViewModelActivity"></activity>
        <activity android:name="com.base.androidbaseframwork.rxjava.RxbusActivity" />
        <activity android:name="com.base.androidbaseframwork.MainActivity" />
        <activity android:name="com.base.androidbaseframwork.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>